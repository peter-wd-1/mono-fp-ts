{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/cicd/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/cicd",
        "tsConfig": "libs/cicd/tsconfig.lib.json",
        "packageJson": "libs/cicd/package.json",
        "main": "libs/cicd/src/index.ts",
        "assets": [
          "libs/cicd/*.md",
          {
            "input": "./libs/cicd/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/cicd/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/cicd",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/cicd",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/cicd/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/cicd"],
      "options": {
        "jestConfig": "libs/cicd/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p libs/cicd/tsconfig.lib.json -w"
      }
    },
    "commit": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn nx make-deploylist cicd && git add . && yarn cz"
      }
    },
    "release": {
      "executor": "nx:run-commands",
      "options": {
        "command": "standard-version && echo released > bumped && git push --follow-tags && bumped"
      }
    },
    "releasetest": {
      "executor": "nx:run-commands",
      "options": {
        "command": "touch bumped"
      }
    },
    "null": {
      "executor": "nx:run-commands",
      "options": {
        "command": ""
      }
    },
    "dev": {
      "executor": "@rfiready/cicd:cd",
      "outputs": ["{options.outputPath}"],
      "options": {
        "env": "dev",
        "action": "dev"
      }
    },
    "deploy-local": {
      "executor": "@rfiready/cicd:cd",
      "outputs": ["{options.outputPath}"],
      "options": {
        "env": "dev",
        "action": "deploy"
      }
    },
    "deploy": {
      "executor": "@rfiready/cicd:cd",
      "outputs": ["{options.outputPath}"],
      "options": {
        "env": "prod",
        "action": "deploy"
      }
    },
    "make-deploylist": {
      "executor": "@rfiready/cicd:cd",
      "outputs": ["{options.outputPath}"],
      "options": {
        "env": "dev",
        "action": "make"
      }
    },
    "push-deploylist": {
      "executor": "@rfiready/cicd:cd",
      "outputs": ["{options.outputPath}"],
      "options": {
        "env": "prod",
        "action": "push"
      }
    },
    "serve-deploylist": {
      "executor": "@rfiready/cicd:cd",
      "outputs": ["{options.outputPath}"],
      "options": {
        "env": "prod",
        "action": "serve"
      }
    }
  },
  "tags": []
}
