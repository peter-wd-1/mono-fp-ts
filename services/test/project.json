{
  "root": "services/test",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "services/test/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/services/test",
        "main": "services/test/src/main.ts",
        "tsConfig": "services/test/tsconfig.app.json",
        "assets": ["services/test/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "services/test/src/environments/environment.ts",
              "with": "services/test/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "test:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "test:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["services/test/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/services/test"],
      "options": {
        "jestConfig": "services/test/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "type-check": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "main": "services/test/src/main.ts",
        "outputPath": "dist/services/test",
        "tsConfig": "services/test/tsconfig.app.json"
      }
    },
    "dev": {
      "executor": "@rfiready/cicd:cd",
      "outputs": ["{options.outputPath}"],
      "option": {
        "env": "development"
      }
    },
    "deploy": {
      "executor": "@rfiready/cicd:cd",
      "outputs": ["{options.outputPath}"],
      "option": {
        "env": "production"
      }
    },
    "docker-dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker compose -f services/test/docker/docker-compose.dev.yml up -d"
      }
    },
    "push-deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo this is for test perpose"
      }
    },
    "mark-deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "mkdir -p tmp/deploy && cp services/test/docker/docker-compose.yml tmp/deploy/docker-compose.test.yml"
      }
    }
  },
  "tags": []
}
